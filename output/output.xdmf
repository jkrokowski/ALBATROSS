          <DataItem Dimensions="11 3" Format="HDF">output.h5:/Function/Displacement/0</DataItem>
        </Attribute>
      </Grid>
    </Grid>
    <Grid Name="Rotation" GridType="Collection" CollectionType="Temporal">
