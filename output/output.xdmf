        <Time Value="0" />
        <Attribute Name="Rotation" AttributeType="Vector" Center="Node">
          <DataItem Dimensions="11 3" Format="HDF">output.h5:/Function/Rotation/0</DataItem>
        </Attribute>
      </Grid>
    </Grid>
  </Domain>
</Xdmf>
